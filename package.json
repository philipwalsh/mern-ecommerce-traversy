{
  "name": "mern-ecommerce",
  "version": "1.0.0",
  "description": "MERN eCommerce fullstack application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js -i",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philipwalsh/mern-ecommerce-traversy.git"
  },
  "keywords": [
    "philip",
    "walsh"
  ],
  "author": "philip walsh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philipwalsh/mern-ecommerce-traversy/issues"
  },
  "homepage": "https://github.com/philipwalsh/mern-ecommerce-traversy#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.11.8"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
